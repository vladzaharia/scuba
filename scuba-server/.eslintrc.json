{
	"extends": ["../.eslintrc.json"],
	"ignorePatterns": ["!**/*", "dist/**/*"],
	"overrides": [
		{
			"files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
			"rules": {}
		},
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {
				"@typescript-eslint/no-non-null-assertion": 0
			}
		},
		{
			"files": ["*.js", "*.jsx"],
			"rules": {}
		}
	]
}
